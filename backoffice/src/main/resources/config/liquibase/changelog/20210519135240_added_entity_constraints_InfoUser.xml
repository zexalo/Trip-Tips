<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">
    <!--
        Added the constraints for entity InfoUser.
    -->
    <changeSet id="20210519135240-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="info_user"
                                 constraintName="fk_info_user__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_user"/>

        <addForeignKeyConstraint baseColumnNames="info_user_id"
                                 baseTableName="rel_info_user__recommendation"
                                 constraintName="fk_rel_info_user__recommendation__info_user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="info_user"/>

        <addForeignKeyConstraint baseColumnNames="recommendation_id"
                                 baseTableName="rel_info_user__recommendation"
                                 constraintName="fk_rel_info_user__recommendation__recommendation_id"
                                 referencedColumnNames="id"
                                 referencedTableName="recommendation"/>
    </changeSet>
</databaseChangeLog>
